{
  "name": "@gae-labs/better-ms",
  "version": "1.0.0",
  "description": "Millisecond conversion tool with support for multiple time units in a single input string",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gae-labs/betterMsJs.git"
  },
  "main": "./index",
  "files": [
    "index.js"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint lib/* bin/*",
    "test": "mocha tests.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "9.12.0",
    "expect.js": "0.3.1",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "mocha": "10.1.0",
    "prettier": "2.0.5"
  },
  "bugs": {
    "url": "https://github.com/gae-labs/betterMsJs/issues"
  },
  "homepage": "https://github.com/gae-labs/betterMsJs#readme",
  "keywords": [
    "ms"
  ],
  "author": "gae-labs"
}